<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="2018033001" author="jerry">
        <comment>用户</comment>
        <createTable tableName="cma_user">
            <column name="id" type="varchar(36)">
                <constraints primaryKey="true" nullable="false" primaryKeyName="pk_cma_user"/>
            </column>
            <column name="create_at" type="datetime" />
            <column name="update_at" type="datetime" />
            <column name="name" type="varchar(36)"/>
            <column name="code" type="varchar(36)" />
        </createTable>
        <rollback>
            <dropTable tableName="cma_user"/>
        </rollback>
    </changeSet>

</databaseChangeLog>